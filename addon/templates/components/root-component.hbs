{{#resolve-content content=all as |resolvedAll|}}

  {{resolvedAll.length}}
  {{#if resolvedAll.length}}
    {{generic-field
      content=(get-parents resolvedAll)
      tree=myTree

      limit=limit
      optionLabelPath=optionLabelPath
      optionValuePath=optionValuePath
      optionChildrenPath=optionChildrenPath
      optionLimitPath=optionLimitPath

      loadMore=(action loadMore)
      searchMore=(action searchMore)
      addSelection=(action 'addSelection')
      removeSelection=(action 'removeSelection')}}
  {{else}}
    loading...
  {{/if}}

{{/resolve-content}}
