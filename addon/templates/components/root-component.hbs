{{#resolve-content content=content as |resolvedContent|}}

  {{log resolvedContent}}

  {{resolvedContent.length}}
  {{#each resolvedContent as |item|}}
    {{item.label}}
  {{/each}}


  {{generic-field
    content=resolvedContent
    tree=myTree

    limit=limit
    optionLabelPath=optionLabelPath
    optionValuePath=optionValuePath
    optionChildrenPath=optionChildrenPath
    optionLimitPath=optionLimitPath

    addSelection=(action 'addSelection')
    removeSelection=(action 'removeSelection')}}

{{/resolve-content}}